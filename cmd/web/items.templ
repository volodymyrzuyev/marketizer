package web

import "github.com/volodymyrzuyev/marketizer/internal/database/sql"

templ Items(items []custSql.Item) {
	<div class="flex flex-wrap" id="content" hx-get="/items" hx-trigger="every 10s" hx-swap="outerHTML" hx-swap-oob="true">
		for _, itemLine := range items {
			@item(itemLine)
		}
	</div>
}
